package Home;

import java.util.Scanner;

public class Modul5{
static int n;
 
 
 
 
   public static void main(String[] args) {
       @SuppressWarnings("resource")
        Scanner s= new Scanner(System.in);
      
   
     n=s.nextInt();      int [] a = new int [n];
     
     for(int i = 0;i<a.length;i++){
         a[i]=s.nextInt();
     }
     
     for(int i = 0;i<a.length;i++){
        if(a[i]%2==0) System.out.print(a[i]+" ");
     } 
 
 
   } 
   }
